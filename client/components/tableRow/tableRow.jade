template(name='tableRow')
    tr
        td(class="userName {{active}}")
            =name
        +each levels
            +each contests
                td.border
                +each problems
                    +userSubmit result=../../../result contest=.. problem=this
            td.border
            td
                =solvedInLevel
            td
                =attemptsInLevel
        td.border
        td
            =solved
        td
            =attempts

