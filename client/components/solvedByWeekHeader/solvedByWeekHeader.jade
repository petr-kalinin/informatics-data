template(name='solvedByWeekHeader')
    tr
        if name
            td
                | &nbsp;
            if admin
                td
                    span(title="Базовый уровень")
                        | БУ
            td
                span(title="Уровень")
                    | У
            td
                span(title="Рейтинг")
                    | Р
            td
                span(title="Активность")
                    | A
        +each weekSet 
            +oneWeekHeader weekNumber=this userList=../userList

template(name='oneWeekHeader')
    td
        =weekHeader
                
                    
    